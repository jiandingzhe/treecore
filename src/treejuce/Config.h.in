#ifndef TREEJUCE_CONFIG_H
#define TREEJUCE_CONFIG_H

// The content in this header is automatically generated during CMake build
// stage. Do not directly copy & use this header across different computers and
// OS.

#define TREEJUCE_CPU_@TREEJUCE_CPU@
#define TREEJUCE_OS_@TREEJUCE_OS@

#define TREEJUCE_SIZE_PTR @CMAKE_SIZEOF_VOID_P@

#if defined _MSC_VER
#   define TREEJUCE_COMPILER_MSVC
#elif defined __GNUC__
#   define TREEJUCE_COMPILER_GCC
#elif defined __clang__
#   define TREEJUCE_COMPILER_CLANG
#elif defined DOXYGEN
#   define TREEJUCE_COMPILER_DOXYGEN
#else // TODO intel icc
#   error "unknown compiler"
#endif

#ifdef TREEJUCE_COMPILER_MSVC
#   if _MSC_VER < 1500
#       define JUCE_VC8_OR_EARLIER 1
#       if _MSC_VER < 1400
#           define JUCE_VC7_OR_EARLIER 1
#           if _MSC_VER < 1300
#               error "VC 6 not supported"
#           endif
#       endif
#   endif
#   if TREEJUCE_SIZE_PTR == 8 || ! JUCE_VC7_OR_EARLIER
#       define JUCE_USE_INTRINSICS 1
#   endif
#endif

#if defined TREEJUCE_CPU_X86
#   define TREEJUCE_ENDIAN_LITTLE
#else
#   error "unsupported CPU"
#endif

#ifndef NDEBUG
#   define JUCE_DEBUG 1
#endif

#endif // TREEJUCE_CONFIG_H
